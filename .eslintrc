{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "jsx-a11y/anchor-is-valid": [ 
      "error", 
      { 
        "components": [ "Link" ], 
        "specialLink": [ "to" ] 
      } 
    ]
  }
}
